ServerAliveInterval 120
ControlPath ~/.ssh/control/%C
#ControlPath ~/.ssh/cm-%r@%h:%p
ControlMaster auto
ControlPersist 10m
ForwardX11 yes
ForwardAgent yes
ForwardX11Trusted yes

host remarkable
    Hostname 10.11.99.1
    User root
    Port 22
	IdentityFile ~/.ssh/id_rsa
	PubkeyAcceptedKeyTypes +ssh-rsa
    HostKeyAlgorithms +ssh-rsa

Host supportshell
    HostName supportshell.cee.redhat.com
    User pbertera

Host *.quicklab.*.cee.redhat.com
   User quicklab
   IdentityFile ~/.ssh/quicklab.key
   UserKnownHostsFile /dev/null
   StrictHostKeyChecking no

Host *.lab.upshift.*.redhat.com
   User quicklab
   IdentityFile ~/.ssh/quicklab.key
   UserKnownHostsFile /dev/null
   StrictHostKeyChecking no

Host *.lab.*.cee.redhat.com
   User quicklab
   IdentityFile ~/.ssh/quicklab.key
   UserKnownHostsFile /dev/null
   StrictHostKeyChecking no

Host wallace
   User pbertera
   HostName 172.16.18.88

Host vm 
	hostname vm
	User pbertera
	ForwardX11 no
	ForwardX11Trusted no
	ProxyCommand ssh -A -W %h:%p -x -o "ForwardX11 no" -o "ForwardX11Trusted no" wallace

Host worker-* 
	User core
	ForwardX11 no
	ForwardX11Trusted no
	ProxyCommand ssh -A -W %h:%p -x -o "ForwardX11 no" -o "ForwardX11Trusted no" wallace


Host lb.lab.bertera.it
   User pbertera
   HostName lb.lab.bertera.it
